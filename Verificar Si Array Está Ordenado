#include <iostream>
using namespace std;

bool estaOrdenado(int arr[], int n){
    for(int i=0;i<n-1;i++)
        if(arr[i]>arr[i+1]) return false;
    return true;
}

int main() {
    int arr[]={1,2,3,4,5}, n=5;
    if(estaOrdenado(arr,n)) cout << "Array ordenado\n";
    else cout << "Array no ordenado\n";
    return 0;
}

