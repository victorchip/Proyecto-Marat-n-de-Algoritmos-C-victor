#include <iostream>
using namespace std;

int main() {
    int mat[2][3]={{1,2,3},{2,2,6}}, m=2, n=3, x=2;
    int contador=0;
    for(int i=0;i<m;i++){
        for(int j=0;j<n;j++){
            if(mat[i][j]==x) contador++;
        }
    }
    cout << "El elemento " << x << " aparece " << contador << " veces.\n"; // 3
    return 0;
}
