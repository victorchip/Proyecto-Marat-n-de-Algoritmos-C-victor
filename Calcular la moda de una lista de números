#include <bits/stdc++.h>
using namespace std;

int main() {
    int n; cin >> n;
    map<int,int> freq;
    for (int i=0;i<n;++i){
        int x; cin >> x;
        freq[x]++;
    }
    int moda=-1, maxf=0;
    for(auto &[num, f]:freq)
        if(f>maxf) maxf=f, moda=num;
    cout << "Moda: " << moda << " (frecuencia " << maxf << ")\n";
}

