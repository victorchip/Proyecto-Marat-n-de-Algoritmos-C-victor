#include <bits/stdc++.h>
using namespace std;

int main(){
    int n; cin >> n;
    vector<int> v(n);
    for(int i=0;i<n;++i) cin >> v[i];
    int best=v[0], curr=0;
    for(int x:v){
        curr = max(x, curr+x);
        best = max(best, curr);
    }
    cout << "Suma mÃ¡xima: " << best << '\n';
}

