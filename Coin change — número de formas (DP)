#include <bits/stdc++.h>
using namespace std;
using ull = unsigned long long;
int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int m, sum; if(!(cin>>m>>sum)) return 0;
    vector<int> coins(m);
    for(int i=0;i<m;++i) cin>>coins[i];
    vector<ull> dp(sum+1,0);
    dp[0]=1;
    for(int c: coins) for(int s=c; s<=sum; ++s) dp[s] += dp[s-c];
    cout<<dp[sum]<<"\n";
    return 0;
}
