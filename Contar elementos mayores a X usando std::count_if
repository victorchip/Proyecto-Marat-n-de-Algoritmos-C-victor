#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main(){
    int n,X; cin>>n>>X;
    vector<int> v(n);
    for(int i=0;i<n;i++) cin>>v[i];

    int cnt = count_if(v.begin(),v.end(),[X](int a){ return a>X; });
    cout<<"Cantidad de elementos mayores que "<<X<<": "<<cnt<<endl;
    return 0;
}
