#include <bits/stdc++.h>
using namespace std;
optional<int> to_int(const string& s){
    try{ size_t pos; int v=stoi(s,&pos); if(pos==s.size()) return v; else return nullopt; }
    catch(...) { return nullopt; }
}
int main(){
    string s; while(cin>>s){
        auto v=to_int(s);
        if(v) cout<<"INT "<<*v<<"\n"; else cout<<"BAD\n";
    }
}
