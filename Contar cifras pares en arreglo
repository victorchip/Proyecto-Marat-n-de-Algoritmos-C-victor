#include <iostream>
using namespace std;

int digitos(int n){ if (n==0) return 1; int c=0; n = (n<0)? -n : n; while(n){ n/=10; ++c; } return c; }

int main() {
    int N; cin >> N;
    int cnt=0, x;
    for (int i=0;i<N;++i){ cin>>x; if (digitos(x)%2==0) ++cnt; }
    cout << "Con dÃ­gitos pares: " << cnt << endl;
    return 0;
}
