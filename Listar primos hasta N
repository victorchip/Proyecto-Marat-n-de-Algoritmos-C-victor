#include <iostream>
using namespace std;

bool esPrimo(int x) {
    if (x <= 1) return false;
    for (int i = 2; i * i <= x; ++i)
        if (x % i == 0) return false;
    return true;
}

int main() {
    int N;
    cout << "Ingresa N: ";
    cin >> N;
    for (int i = 2; i <= N; ++i)
        if (esPrimo(i)) cout << i << " ";
    cout << endl;
    return 0;
}

