#include <iostream>
using namespace std;

int main() {
    int mat[2][3]={{1,2,3},{4,5,6}};
    int m=2, n=3, x=5;
    bool encontrado=false;
    for(int i=0;i<m;i++){
        for(int j=0;j<n;j++){
            if(mat[i][j]==x) encontrado=true;
        }
    }
    cout << (encontrado ? "Elemento encontrado\n" : "Elemento no encontrado\n"); // Encontrado
    return 0;
}
