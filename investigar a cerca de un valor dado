#include <bits/stdc++.h>
using namespace std;

int main(){
    int n,x; cin >> n;
    vector<int> v(n);
    for (int i=0;i<n;++i) cin >> v[i];
    sort(v.begin(), v.end());
    cin >> x;

    auto it = lower_bound(v.begin(), v.end(), x);
    if(it == v.begin()) cout << *it;
    else if(it == v.end()) cout << v.back();
    else {
        int a = *(it-1), b = *it;
        cout << (abs(a-x) <= abs(b-x) ? a : b);
    }
}

