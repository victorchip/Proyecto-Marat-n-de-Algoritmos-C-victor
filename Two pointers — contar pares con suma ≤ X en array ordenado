#include <bits/stdc++.h>
using namespace std;
int main(){
    int n; if(!(cin>>n)) return 0;
    vector<long long> a(n); for(int i=0;i<n;++i) cin>>a[i];
    sort(a.begin(), a.end());
    long long X; cin>>X; long long cnt=0;
    int l=0, r=n-1;
    while(l<r){
        if(a[l]+a[r]<=X){ cnt += (r-l); ++l; }
        else --r;
    }
    cout<<cnt<<"\n";
}

