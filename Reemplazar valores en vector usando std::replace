#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main(){
    int n,oldVal,newVal; cin>>n>>oldVal>>newVal;
    vector<int> v(n);
    for(int i=0;i<n;i++) cin>>v[i];

    replace(v.begin(),v.end(),oldVal,newVal);

    cout<<"Vector despuÃ©s de reemplazo: ";
    for(int x:v) cout<<x<<" ";
    cout<<endl;
    return 0;
}
