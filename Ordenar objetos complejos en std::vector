#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

struct Estudiante{
    string nombre;
    int nota;
};

int main(){
    int n; cin>>n;
    vector<Estudiante> v(n);
    for(int i=0;i<n;i++) cin>>v[i].nombre>>v[i].nota;

    sort(v.begin(),v.end(),[](Estudiante a,Estudiante b){ return a.nota>b.nota; });

    for(auto e:v) cout<<e.nombre<<" "<<e.nota<<endl;
    return 0;
}
