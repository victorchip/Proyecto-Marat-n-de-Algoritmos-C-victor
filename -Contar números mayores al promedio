#include <iostream>
using namespace std;

int main() {
    int N; cin >> N;
    double a[1000], suma=0;
    for (int i=0;i<N;++i){ cin>>a[i]; suma+=a[i]; }
    double prom = suma / N;
    int cnt=0;
    for (int i=0;i<N;++i) if (a[i] > prom) ++cnt;
    cout << "Mayores al promedio: " << cnt << endl;
    return 0;
}

