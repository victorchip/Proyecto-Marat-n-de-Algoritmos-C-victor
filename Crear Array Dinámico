#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Ingresa tamaÃ±o del array: ";
    cin >> n;
    int *arr = new int[n];
    for(int i=0;i<n;i++){
        cout << "Elemento " << i << ": ";
        cin >> arr[i];
    }
    cout << "Elementos ingresados: ";
    for(int i=0;i<n;i++) cout << arr[i] << " ";
    cout << endl;
    delete[] arr;
    return 0;
}

