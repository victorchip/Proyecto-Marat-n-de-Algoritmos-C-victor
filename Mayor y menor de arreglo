#include <iostream>
using namespace std;

int main() {
    int N; cin >> N;
    int x, maxv, minv;
    for (int i=0;i<N;++i) {
        cin >> x;
        if (i==0){ maxv=minv=x; }
        else { if (x>maxv) maxv=x; if (x<minv) minv=x; }
    }
    cout << "Mayor: " << maxv << " Menor: " << minv << endl;
    return 0;
}

