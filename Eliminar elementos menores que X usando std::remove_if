#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main(){
    int n,X; cin>>n>>X;
    vector<int> v(n);
    for(int i=0;i<n;i++) cin>>v[i];

    v.erase(remove_if(v.begin(),v.end(),[X](int a){ return a<X; }), v.end());

    cout<<"Vector despuÃ©s de eliminar elementos menores que "<<X<<": ";
    for(int x:v) cout<<x<<" ";
    cout<<endl;
    return 0;
}
