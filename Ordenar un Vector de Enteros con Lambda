#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    int n;
    cout << "Ingrese tamaÃ±o del vector: ";
    cin >> n;
    vector<int> v(n);
    cout << "Ingrese elementos:\n";
    for(int i=0;i<n;i++) cin >> v[i];

    sort(v.begin(), v.end(), [](int a,int b){ return a>b; });

    cout << "Vector ordenado descendente: ";
    for(int x:v) cout << x << " ";
    cout << endl;
    return 0;
}
