#include <bits/stdc++.h>
using namespace std;
int main(){
    long long x; if(!(cin>>x)) return 0;
    if(x<0 || (x%10==0 && x!=0)){ cout<<"NO\n"; return 0; }
    long long rev=0;
    while(x>rev){
        rev = rev*10 + x%10;
        x /= 10;
    }
    cout << (x==rev || x==rev/10 ? "YES\n" : "NO\n");
}
