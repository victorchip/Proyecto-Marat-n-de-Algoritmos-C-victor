#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Ingresa N: ";
    cin >> n;
    if (n <= 1) { cout << "No primo\n"; return 0; }
    bool primo = true;
    for (int i = 2; i * i <= n; ++i) {
        if (n % i == 0) { primo = false; break; }
    }
    cout << (primo ? "Primo\n" : "No primo\n");
    return 0;
}

