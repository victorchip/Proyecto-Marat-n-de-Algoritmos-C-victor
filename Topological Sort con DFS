#include <bits/stdc++.h>
using namespace std;

vector<vector<int>> g;
vector<int> vis, order;

void dfs(int u){
    vis[u]=1;
    for(int v:g[u]) if(!vis[v]) dfs(v);
    order.push_back(u);
}

int main(){
    int n,m; cin>>n>>m;
    g.resize(n);
    for(int i=0;i<m;++i){int u,v;cin>>u>>v;g[u].push_back(v);}
    vis.assign(n,0);
    for(int i=0;i<n;++i) if(!vis[i]) dfs(i);
    reverse(order.begin(), order.end());
    for(int x:order) cout<<x<<" ";
}

