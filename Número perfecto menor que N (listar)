#include <iostream>
using namespace std;

bool esPerfecto(int n) {
    int suma = 0;
    for (int i=1;i<=n/2;++i) if (n%i==0) suma += i;
    return suma == n;
}

int main() {
    int N; cout << "N: "; cin >> N;
    for (int i=1;i<=N;++i) if (esPerfecto(i)) cout << i << " ";
    cout << endl;
    return 0;
}

