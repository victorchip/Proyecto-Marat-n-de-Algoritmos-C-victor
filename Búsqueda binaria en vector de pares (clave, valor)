#include <bits/stdc++.h>
using namespace std;
int main(){
    int n; if(!(cin>>n)) return 0;
    vector<pair<int,string>> v(n);
    for(int i=0;i<n;++i) cin>>v[i].first>>v[i].second;
    sort(v.begin(), v.end());
    int key; cin>>key;
    auto it = lower_bound(v.begin(), v.end(), make_pair(key, string("")));
    if(it!=v.end() && it->first==key) cout<<it->second<<"\n";
    else cout<<"NO\n";
}
