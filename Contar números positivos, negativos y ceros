#include <iostream>
using namespace std;

int main() {
    int N; cout << "N: "; cin >> N;
    int pos=0, neg=0, cero=0, x;
    for (int i=0;i<N;++i) {
        cin >> x;
        if (x>0) ++pos;
        else if (x<0) ++neg;
        else ++cero;
    }
    cout << "Pos: " << pos << " Neg: " << neg << " Ceros: " << cero << endl;
    return 0;
}


