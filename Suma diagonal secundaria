#include <iostream>
using namespace std;

int main() {
    int N; cout << "N: "; cin >> N;
    int a[100][100];
    for (int i=0;i<N;++i) for (int j=0;j<N;++j) cin >> a[i][j];
    int suma = 0;
    for (int i=0;i<N;++i) suma += a[i][N-1-i];
    cout << "Suma diagonal secundaria: " << suma << endl;
    return 0;
}


