#include <iostream>
#include <string>
using namespace std;

int main() {
    string a, b;
    cout << "Linea A: "; getline(cin, a);
    cout << "Linea B: "; getline(cin, b);
    if (a.size() == b.size()) cout << "Iguales\n";
    else if (a.size() > b.size()) cout << "A es más larga\n";
    else cout << "B es más larga\n";
    return 0;
}
