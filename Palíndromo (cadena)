#include <iostream>
#include <string>
using namespace std;

int main() {
    string s;
    cout << "Ingresa cadena: ";
    cin >> s;
    bool pal = true;
    for (int i=0, j=s.size()-1; i<j; ++i, --j) if (s[i]!=s[j]) { pal=false; break; }
    cout << (pal ? "Es palíndromo\n" : "No es palíndromo\n");
    return 0;
}

