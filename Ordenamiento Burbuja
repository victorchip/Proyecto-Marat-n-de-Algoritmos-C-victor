#include <iostream>
using namespace std;

int main(){
    int arr[]={5,3,8,1,2}, n=5;
    cout << "Antes: ";
    for(int i=0;i<n;i++) cout << arr[i] << " ";
    cout << endl;

    for(int i=0;i<n-1;i++)
        for(int j=0;j<n-1-i;j++)
            if(arr[j]>arr[j+1]) swap(arr[j],arr[j+1]);

    cout << "DespuÃ©s: ";
    for(int i=0;i<n;i++) cout << arr[i] << " ";
    cout << endl;

    return 0;
}

