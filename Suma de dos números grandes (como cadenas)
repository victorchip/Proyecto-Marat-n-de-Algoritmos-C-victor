#include <iostream>
#include <string>
using namespace std;

int main() {
    string a, b;
    cin >> a >> b;
    string res = "";
    int i = a.size()-1, j = b.size()-1, carry=0;
    while (i>=0 || j>=0 || carry) {
        int da = (i>=0 ? a[i--]-'0' : 0);
        int db = (j>=0 ? b[j--]-'0' : 0);
        int s = da + db + carry;
        res.push_back(char('0' + (s%10)));
        carry = s/10;
    }
    reverse(res.begin(), res.end());
    cout << res << endl;
    return 0;
}

