#include <bits/stdc++.h>
using namespace std;
int main(){
    int n; if(!(cin>>n)) return 0;
    vector<int>a(n); for(int i=0;i<n;++i)cin>>a[i];
    int X; cin>>X;
    unordered_map<int,int> mp;
    for(int i=0;i<n;++i){
        int need = X - a[i];
        if(mp.count(need)){ cout<<mp[need]<<" "<<i<<"\n"; return 0; }
        mp[a[i]] = i;
    }
    cout<<"NO\n";
}
