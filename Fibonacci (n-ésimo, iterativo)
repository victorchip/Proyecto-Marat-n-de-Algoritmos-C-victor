#include <iostream>
using namespace std;

int main() {
    int N;
    cout << "Ingresa N (>=0): ";
    cin >> N;
    if (N == 0) { cout << "0\n"; return 0; }
    long long a = 0, b = 1;
    for (int i = 2; i <= N; ++i) {
        long long c = a + b;
        a = b;
        b = c;
    }
    cout << (N==1 ? 1 : b) << endl;
    return 0;
}
